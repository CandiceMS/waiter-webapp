
  <h4>Here are all waiter shifts for the week:</h4>
  <div class="renderAdmin">
    <table>
        <tr>
            {{#each waiter_shifts}}
            <th>
                {{day}}
            </th>
            {{/each}}
        </tr>
        <tr>
            {{#each waiter_shifts}}
            <td>
                <ul>
                    {{#each waiter_name}}
                    {{.}}
                    {{/each}}
                </ul>
            </td>
            {{/each}}
        </tr>
    </table>
</div>